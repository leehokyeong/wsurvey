	</parent>

	<groupId>com.woongjin.pjt</groupId>
	<artifactId>wsurvey-web</artifactId>
	<packaging>jar</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>wsurvey-web</name>
	<description>wsurvey-web</description>

	<dependencies>

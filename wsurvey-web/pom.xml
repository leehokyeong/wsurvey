			<artifactId>spring-boot-starter-thymeleaf</artifactId>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<mainClass>com.woongjin.pjt.wsurveyweb.WsurveyWebApplication</mainClass>
				</configuration>
			</plugin>
		</plugins>
	</build>

